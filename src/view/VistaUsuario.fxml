<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.UserViewController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <TextArea fx:id="fieldExpression" layoutX="71.0" layoutY="48.0" onMouseClicked="#validateCaret" prefHeight="91.0" prefWidth="477.0" promptText="Ingrese la formula #1">
               <font>
                  <Font size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
               <effect>
                  <InnerShadow color="#cccccc" />
               </effect>
            </TextArea>
            <TitledPane animated="false" expanded="false" layoutX="71.0" layoutY="166.0" text="Operadores">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="243.0">
                     <children>
                        <GridPane alignment="TOP_CENTER" layoutX="38.0" layoutY="18.0" prefHeight="144.0" prefWidth="215.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Button fx:id="botonNegar" mnemonicParsing="false" onAction="#asignarNegar" prefHeight="25.0" prefWidth="64.0" text="~()" />
                              <Button fx:id="botonAnd" mnemonicParsing="false" onAction="#asignarAnd" prefHeight="25.0" prefWidth="64.0" text="()^()" GridPane.rowIndex="1" />
                              <Button fx:id="conditional" mnemonicParsing="false" onAction="#asignarConditional" prefHeight="25.0" prefWidth="64.0" text="()-&gt;()" GridPane.rowIndex="2" />
                              <Button fx:id="botonOr" mnemonicParsing="false" onAction="#asignarOr" prefHeight="25.0" prefWidth="58.0" text="()v()" GridPane.columnIndex="1" />
                              <Button fx:id="botonBionditional" mnemonicParsing="false" onAction="#asignarBiconditional" prefHeight="25.0" prefWidth="58.0" text="()&lt;-&gt;()" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                           </children>
                        </GridPane>
                     </children>
                  </AnchorPane>
              </content>
            </TitledPane>
            <Button fx:id="delete" layoutX="352.0" layoutY="244.0" mnemonicParsing="false" prefHeight="14.0" prefWidth="94.0" text="Eliminar" />
            <Button fx:id="previous" layoutX="491.0" layoutY="166.0" mnemonicParsing="false" text="&lt;" />
            <Button fx:id="next" layoutX="522.0" layoutY="166.0" mnemonicParsing="false" text="&gt;" />
            <Button fx:id="verificarValidez" layoutX="359.0" layoutY="330.0" mnemonicParsing="false" onAction="#verificarVal" prefHeight="25.0" prefWidth="197.0" text="Verificar validez" />
            <Button layoutX="352.0" layoutY="211.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="94.0" text="Ver" />
            <Button layoutX="457.0" layoutY="211.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="94.0" text="Editar" />
            <Label layoutX="352.0" layoutY="170.0" prefHeight="17.0" prefWidth="94.0" text="Formula # 1" />
            <Button layoutX="457.0" layoutY="244.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="94.0" text="Guardar" />
         </children>
      </AnchorPane>
   </children>
</Group>
